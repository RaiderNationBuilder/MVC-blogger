<div>
    <h1>Sign up</h1>
    <h2> Username</h2>
    <input id="signUpUser" />
    <h2> Email</h2>
    <input id="signUpEmail" />
    <h2> Password</h2>
    <input id="signUpPassword" />
    <button id="signUpButton">Save</button>
</div>

<div>
    <h1>Login</h1>
    <h2> Username</h2>
    <input />
    <h2> Email</h2>
    <input />
    <h2> Password</h2>
    <input />
    <button>Save</button>
</div>


<script>
    $("#signUpButton").on("click", function () {

        var newUser = {
            username: $('#signUpUser').val(),
            email: $('#signUpEmail').val(),
            password: $('#signUpPassword').val(),
        }
        console.log("you got clicked", newUser)

        fetch("/users", {
            method: "POST",
               headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify(newUser)
        }).then(function(response) {
            return response.json()
        }).then(function(data) {
            console.log('DATA!!!', data)
        })

    })

</script>